    @media (min-width: 900px) {
        .navbar-nav > li > a {
            padding-left: 12px;
            padding-right: 12px;
        }
    }

    @media (max-width: 942px) {
        .categoryDisplayName {
            text-align: center;
            margin-bottom: 30px;

        }
    }

    .fw {
        font-weight: 300
    }

    .loginHeader {
        font-size:18px;
    }

    .navbar-default .navbar-nav > li > a {
        font-size:14px;
    }