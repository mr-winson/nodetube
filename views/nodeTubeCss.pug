// doing this because otherwise it's too annoying with cloudflare caching
style.
    .navbar-default .navbar-nav > li > a {
        color: white;
    }

    * {
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    }

    .view-header {
        display: inline;
    }

    /* 70 : 39 aspect ratio */

    .border-preview {
        border: 1px solid black;
    }

    .upload-title {
        height: 89px;
        overflow: hidden;
        display: flex;
        display: -webkit-flex;
        width: 80%;
        margin: 0 auto;
    }

    .title-anchor:hover {
        text-decoration: none;
        color: red;
    }

    .title-anchor {
        text-decoration: none;
    }

    .upload-title-text {
        max-height: 53px;
        color: black;
        font-size: 20px;
        text-align: center;
        margin: 0;
        margin-top: 8px;
        overflow: hidden;

        font-weight: 300;
    }

    .upload-title-text:hover {
        text-decoration: none
        /*color:black;*/
    }

    .upload-user-text {
        color: black;
        /*text-decoration:underline;*/
    }

    .upload-user-text:hover {
        color: black;
        /*text-decoration:underline;*/
    }

    .search-upload-buttons-container {
        display: grid;
        justify-content: center;
        grid-gap: 10px;
    }

    @media (min-width: 480px) {
        .search-upload-buttons-container {
            grid-auto-flow: column;
        }
    }

    .twenty-six {
        font-size: 26px;
    }

    .upload-details {
        width: 90%;
        margin: 0 auto;
        margin-top: -12px;
    }

    .views {
        height: 30px;
        display: flex;
        display: -webkit-flex;
    }

    .views-text {
        font-size: 14px;
        text-align: center;
        margin: auto;
        color: rgba(0, 0, 0, 0.7);
    }

    .upload-file {
        margin-right: 167px;
        margin-bottom: 20px;
        width: 490px;
    }

    .change-size-button {
        display: inline-block;
        margin: 10px;
    }

    .unclicked-emoji img {
        opacity: 0.5;
    }

    /*.centerDropdown {*/
    /*left: auto !important;*/
    /*right: -25% !important;*/
    /*}*/
    /*.centerDropdown:after {*/
    /*left: auto !important;*/
    /*right: 45% !important;*/
    /*}*/

    .dropdown-menu > li > a {
        color: #333333;
    }

    .vjs-slider {
        height: 15px;
    }

    .plyr.plyr--video {
        max-width: 100%;
        margin: 0 auto;
        border-radius: 5px;
    }

    .plyr.plyr__video-wrapper {
        background: white;
    }

    .navbar-fixed-top .navbar-collapse, .navbar-fixed-bottom .navbar-collapse {
        max-height: 381px;
    }

    /*.body {
        background-color: white;
    }*/

    .preview-image {
        width: inherit;
        max-width: 324px;
        height: 200px;
        max-height: 200px;
        border-radius: 7px;
    }

    /* translate thing to make it centered even with overflow*/
    @media (max-width: 347px) {
        .preview-image {
            max-width: 280px;
            margin-left: 50%;
            transform: translateX(-50%);
        }
    }

    @media (min-width: 900px) {
        .navbar-left {
            float: left !important;
        }

        .navbar-right {
            float: right !important;
            margin-right: -95px;
        }

        .navbar-right ~ .navbar-right {
            margin-right: 0;
        }
    }

    @media (min-width: 1042px) {
        .navbar-right {
            margin-right: -28px;
        }
    }

